{
    "name": "Your OCR",
    "version": "1.0",
    "description": "A chrome extension to extract text from images",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icons/ocr-icon-128.png",
        "default_title": "Click here to take Screenshot of page to OCR."
    },
    "icons": {
        "16": "icons/ocr-icon-16.png",
        "32": "icons/ocr-icon-32.png",
        "64": "icons/ocr-icon-64.png",
        "128": "icons/ocr-icon-128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "manifest_version": 2
}